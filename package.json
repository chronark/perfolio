    "@hookform/resolvers": "^2.8.2",
    "@prisma/client": "^3.3.0",
    "@radix-ui/react-accessible-icon": "^0.1.1",
    "@radix-ui/react-avatar": "^0.1.1",
    "@radix-ui/react-dialog": "^0.1.1",
    "@radix-ui/react-id": "^0.1.1",
    "@radix-ui/react-tooltip": "^0.1.1",
    "classnames": "^2.3.1",
