{
  "name": "perfolio",
  "version": "1.0.0",
  "scripts": {
    "dev": "blitz dev",
    "prebuild": "yarn install && blitz prisma generate",
    "build": "blitz build",
    "start": "blitz start",
    "studio": "blitz prisma studio",
    "fmt": "yarn lint && yarn format",
    "lint": "eslint --ignore-path .gitignore  --ext .js,.ts,.tsx .",
    "format": "prettier --loglevel warn --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "husky install"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "prettier": {
    "semi": false,
    "printWidth": 80,
    "singleQuote": false,
    "trailingComma": "all"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@headlessui/react": "1.2.0",
    "@heroicons/react": "1.0.1",
    "@hookform/resolvers": "2.5.2",
    "@prisma/client": "2.23.0",
    "@radix-ui/react-tooltip": "0.0.18",
    "@sentry/browser": "6.5.0",
    "@sentry/integrations": "6.4.1",
    "@sentry/node": "6.5.0",
    "@sentry/webpack-plugin": "1.15.1",
    "@tailwindcss/jit": "0.1.18",
    "@types/preview-email": "2.0.0",
    "@types/react": "17.0.8",
    "@zeit/next-source-maps": "0.0.3",
    "autoprefixer": "10.2.6",
    "blitz": "0.36.3",
    "classnames": "2.3.1",
    "eslint": "7.27.0",
    "husky": "6.0.0",
    "lint-staged": "11.0.0",
    "next-themes": "0.0.14",
    "postcss": "8.3.0",
    "prettier": "2.3.0",
    "prettier-plugin-prisma": "0.10.1",
    "pretty-quick": "3.1.0",
    "preview-email": "3.0.4",
    "prisma": "~2.23",
    "react": "17.0.2",
    "react-dom": "0.0.0-experimental-6a589ad71",
    "react-error-boundary": "3.1.3",
    "react-hook-form": "7.7.1",
    "recharts": "2.0.9",
    "tailwindcss": "2.1.2",
    "tailwindcss-textshadow": "2.1.3",
    "tslog": "3.2.0",
    "typescript": "~4.3",
    "uuid": "8.3.2",
    "zod": "1.11.17"
  },
  "private": true
}
