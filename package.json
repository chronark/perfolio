  "scripts": {
    "generate-apollo-hooks": "nx build api-graphql",
    "predev": "pnpm generate-apollo-hooks",
    "dev": "concurrently \"nx serve api --port=8080\" \"nx serve app --port=3000\"",
    "prebuild": "pnpm generate-apollo-hooks",
    "build": "nx run-many --target=build --all --with-deps",
    "test": "nx run-many --target=test --all",
    "fmt": "pnpm lint && pnpm prettier --loglevel=warn --write .",
    "lint": "nx workspace-lint && nx lint && pnpm lint:graphql",
    "lint:graphql": "graphql-schema-linter -c .",
    "e2e": "nx run-many --target=e2e --all",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest && pnpm install --no-frozen-lockfile && pnpm nx migrate --run-migrations && rm migrations.json",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "tsc": "tsc -p tsconfig.base.json --pretty",
    "build:storybook": "nx build-storybook ui-components",
    "prepare": "husky install",
    "db:migrate": "prisma migrate dev --schema=libs/integrations/prisma/schema.prisma",
    "db:studio": "prisma studio --schema=libs/integrations/prisma/schema.prisma",
    "db:push": "prisma db push --schema=libs/integrations/prisma/schema.prisma"
