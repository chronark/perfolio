{
  "name": "perfolio",
  "version": "1.0.0",
  "scripts": {
    "dev": "blitz dev",
    "build": "blitz build",
    "start": "blitz start",
    "fmt": "yarn lint && yarn format",
    "lint": "eslint --ignore-path .gitignore  --ext .js,.ts,.tsx .",
    "format": "prettier --loglevel warn --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "check:all": "yarn install && yarn fmt && yarn build && lhci autorun",
    "prepare": "husky install"
  },
  "prettier": {
    "semi": false,
    "printWidth": 80,
    "singleQuote": false,
    "trailingComma": "all"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@headlessui/react": "1.2.0",
    "@heroicons/react": "1.0.1",
    "@hookform/resolvers": "2.5.2",
    "@radix-ui/react-tooltip": "0.0.18",
    "@tailwindcss/jit": "0.1.18",
    "@tailwindcss/typography": "0.4.1",
    "@types/preview-email": "2.0.0",
    "@types/react": "17.0.9",
    "autoprefixer": "10.2.6",
    "blitz": "0.36.3",
    "classnames": "2.3.1",
    "eslint": "7.28.0",
    "faunadb": "4.3.0",
    "husky": "6.0.0",
    "lint-staged": "11.0.0",
    "next-themes": "0.0.14",
    "postcss": "8.3.0",
    "prettier": "2.3.0",
    "pretty-quick": "3.1.0",
    "preview-email": "3.0.4",
    "react": "17.0.2",
    "react-dom": "0.0.0-experimental-6a589ad71",
    "react-error-boundary": "3.1.3",
    "react-hook-form": "7.7.1",
    "recharts": "2.0.9",
    "slugify": "1.5.3",
    "tailwindcss": "2.1.3",
    "tailwindcss-textshadow": "2.1.3",
    "tslog": "3.2.0",
    "typescript": "4.2.3",
    "uuid": "8.3.2",
    "zod": "3.1.0"
  },
  "private": true,
  "devDependencies": {
    "@lhci/cli": "0.7.2",
    "fauna-schema-migrate": "2.1.2",
    "vercel": "23.0.0"
  }
}
