{
  "name": "perfolio",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "generate-apollo-hooks": "nx build api-graphql",
    "predev": "pnpm generate-apollo-hooks",
    "prebuild": "pnpm prisma generate && pnpm generate-apollo-hooks",
    "fmt": "pnpm lint && pnpm format",
    "format": "prettier --loglevel=warn --write .",
    "tsc": "tsc --pretty",
    "prepare": "husky install",
    "db:migrate": "prisma migrate dev --schema=pkg/integrations/prisma/schema.prisma",
    "db:studio": "prisma studio --schema=pkg/integrations/prisma/schema.prisma",
    "db:push": "prisma db push --schema=pkg/integrations/prisma/schema.prisma"
  },
  "dependencies": {
