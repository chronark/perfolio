          "options": {
            "lintFilePatterns": ["libs/time/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/time"],
          "options": {
            "jestConfig": "libs/time/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "iexcloud": {
      "root": "libs/iexcloud",
      "sourceRoot": "libs/iexcloud/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/iexcloud/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/iexcloud"],
          "options": {
            "jestConfig": "libs/iexcloud/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "landing": {
      "root": "apps/landing",
      "sourceRoot": "apps/landing",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/landing",
            "outputPath": "dist/apps/landing"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "landing:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "landing:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "landing:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/landing"],
          "options": {
            "jestConfig": "apps/landing/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
