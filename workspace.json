          }
        }
      }
    },
    "email": {
      "root": "libs/email",
      "sourceRoot": "libs/email/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/email/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/email"],
          "options": {
            "jestConfig": "libs/email/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
