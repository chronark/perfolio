        },
        "lhci": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "pnpm nx build app --prod && pnpm lhci autorun --config=./apps/app/lighthouserc.js"
              }
            ]
          }
        },
